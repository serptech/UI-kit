{
  "name": "@serp/ui-kit",
  "version": "0.0.1",
  "description": "UI kit for SERP web apps",
  "author": "SERP development",
  "license": "MIT",
  "browser": "./src/index.js",
  "module": "./src/index.js",
  "files": [
    "src/*",
    "!/**/__tests__"
  ],
  "scripts": {
    "test": "better-npm-run test",
    "test:watch": "better-npm-run test:watch",
    "test:coverage": "better-npm-run test:coverage",
    "lint": "eslint src",
    "prettier": "prettier \"**/*.+(js|jsx|json)\"",
    "stylelint": "stylelint \"src/**/*.+(js|jsx|css)\"",
    "format": "npm run prettier -- --write && npm run stylelint -- --fix",
    "validate": "npm run lint && npm run test && npm run prettier -- --list-different",
    "storybook": "start-storybook",
    "build-storybook": "build-storybook -c .storybook -o docs"
  },
  "betterScripts": {
    "test": {
      "command": "jest",
      "env": {
        "NODE_ENV": "test"
      }
    },
    "test:watch": {
      "command": "jest --watch",
      "env": {
        "NODE_ENV": "test"
      }
    },
    "test:coverage": {
      "command": "jest --collectCoverage",
      "env": {
        "NODE_ENV": "test"
      }
    }
  },
  "peerDependencies": {
    "formik": ">=2.0",
    "react": ">=16.9",
    "react-dom": ">=16.9",
    "react-router-dom": ">=5.0",
    "prop-types": ">=15.7",
    "styled-components": ">=5.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.15.4",
    "@reach/tabs": "^0.11.2",
    "@tippyjs/react": "^4.2.5",
    "browser-cookie-lite": "^1.0.4",
    "date-fns": "^2.25.0",
    "dayjs": "^1.10.7",
    "downshift": "^5.4.7",
    "i18next": "^19.9.2",
    "jump.js": "^1.0.2",
    "lodash-es": "^4.17.21",
    "match-sorter": "^4.2.1",
    "nouislider": "^14.7.0",
    "nouislider-react": "^3.4.1",
    "rc-notification": "^4.5.7",
    "react-contenteditable": "^3.3.6",
    "react-datepicker": "^2.16.0",
    "react-dropzone": "^10.2.2",
    "react-i18next": "^11.12.0",
    "react-qr-svg": "^2.4.0",
    "react-transition-group": "^4.4.2",
    "react-use": "^15.3.4",
    "react-useportal": "^1.0.14",
    "styled-icons": "10.45.0",
    "styled-normalize": "^8.0.7",
    "styled-theming": "^2.2.0",
    "uuid-validate": "0.0.3",
    "yup": "^0.28.5"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-react": "^7.14.5",
    "@storybook/addon-actions": "^5.3.21",
    "@storybook/addon-knobs": "^5.3.21",
    "@storybook/react": "^5.3.21",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^10.4.9",
    "@testing-library/user-event": "^12.8.3",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.5.1",
    "babel-loader": "^8.1.0",
    "babel-plugin-dynamic-import-node": "^2.3.3",
    "better-npm-run": "^0.1.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^3.0.0",
    "formik": "^2.2.9",
    "husky": "^4.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.5.4",
    "jest-environment-jsdom": "^25.5.0",
    "jest-environment-jsdom-global": "^2.0.4",
    "jest-styled-components": "^7.0.5",
    "lint-staged": "^10.5.4",
    "mockdate": "^2.0.5",
    "prettier": "^2.4.1",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.3.0",
    "storybook-react-router": "^1.0.8",
    "styled-components": "^5.3.1",
    "stylelint": "^13.13.1",
    "stylelint-config-recommended": "^3.0.0",
    "test-data-bot": "^0.8.0"
  }
}
